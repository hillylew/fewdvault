{
  "name": "11ty-starter",
  "version": "1.0.0",
  "scripts": {
    "build:css": "tailwindcss -i ./css/main.css -o ../public/main.css --minify",
    "build:site": "ELEVENTY_ENV=production eleventy",
    "watch:css": "tailwindcss -i ./css/main.css -o ../public/main.css --watch",
    "watch:site": "eleventy --serve",
    "build": "npm run clean && npm run build:css && npm run build:site",
    "clean": "rimraf ../public && rimraf ../.cache",
    "start": "npm run clean && concurrently 'npm:watch:*'"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@portabletext/to-html": "^2.0.0",
    "@sanity/client": "^5.2.2",
    "@sanity/image-url": "^1.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "concurrently": "^7.6.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.10",
    "netlify-cli": "^13.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.7"
  },
  "dependencies": {
    "@alpinejs/focus": "^3.11.1",
    "alpinejs": "^3.11.1",
    "groq": "^3.5.1"
  },
  "browserslist": [
    "defaults"
  ]
}
